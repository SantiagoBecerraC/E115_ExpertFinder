[run]
source = ./
omit = 
    */tests/*
    */__pycache__/*
    */.venv/*
    */venv/*
    */.env/*
    */site-packages/*
    */__init__.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError

[html]
directory = docs/coverage/html_report
