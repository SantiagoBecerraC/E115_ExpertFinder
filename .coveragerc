[run]
source = backend
omit = 
    */tests/*
    */venv/*
    */site-packages/*
    */__pycache__/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError
    
[html]
directory = reports/coverage-report
